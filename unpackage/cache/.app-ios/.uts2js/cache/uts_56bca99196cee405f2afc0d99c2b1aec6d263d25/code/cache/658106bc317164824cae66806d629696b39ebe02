{"code":"// @ts-nocheck\nimport { isFunction } from '../isFunction';\nimport { isObject } from '../isObject';\n/**\n * 检查一个值是否为 Promise 类型\n * @param val 要检查的值\n * @returns 如果值的类型是 Promise 类型，则返回 true；否则返回 false\n */\nexport const isPromise = (val) => {\n    // 使用 isObject 函数判断值是否为对象类型\n    // 使用 isFunction 函数判断值是否具有 then 方法和 catch 方法\n    return isObject(val) && isFunction(val.then) && isFunction(val.catch);\n};\n//# sourceMappingURL=/Users/xyhc/Documents/veepai_uniappx/uni_modules/lime-shared/isPromise/index.js.map","references":["/Users/xyhc/Documents/veepai_uniappx/uni_modules/lime-shared/isFunction/index.ts","/Users/xyhc/Documents/veepai_uniappx/uni_modules/lime-shared/isObject/index.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"index.js\",\"sourceRoot\":\"\",\"sources\":[\"index.ts\"],\"names\":[],\"mappings\":\"AAAA,cAAc;AACd,OAAO,EAAC,UAAU,EAAC,MAAM,eAAe,CAAA;AACxC,OAAO,EAAC,QAAQ,EAAC,MAAM,aAAa,CAAA;AACpC;;;;GAIG;AAEH,MAAM,CAAC,MAAM,SAAS,GAAG,CAAU,GAAY;IAC7C,2BAA2B;IAC3B,4CAA4C;IAC5C,OAAO,QAAQ,CAAC,GAAG,CAAC,IAAI,UAAU,CAAC,GAAG,CAAC,IAAI,CAAC,IAAI,UAAU,CAAC,GAAG,CAAC,KAAK,CAAC,CAAC;AACxE,CAAC,CAAC\"}"}
