{"code":"import { defineComponent } from \"vue\";\nimport {} from 'vue';\n/**\n * Radio 单选框\n * @description Radio 单项选择器，需结合fui-radio-group和fui-radio组件一起使用。\n * @tutorial https://unix.firstui.cn/\n * @property {String} name {String} 单项选择器名称\n * @property {String} modelValue {String} 选中的value值，默认值\n * @event {Function} change 选中项发生变化时触发，(event: string) => void\n * @event {Function} update:modelValue 用于组件双向绑定，(event: string) => void\n */\nexport default defineComponent({\n    name: \"fui-radio-group\",\n    emits: ['change', 'update:modelValue'],\n    props: {\n        name: {\n            type: String,\n            default: ''\n        },\n        modelValue: {\n            type: String,\n            default: ''\n        }\n    },\n    data() {\n        return {\n            val: '',\n            childrens: [],\n            fuiForm: null\n        };\n    },\n    watch: {\n        modelValue(val) {\n            this.modelChange(val);\n        }\n    },\n    created() {\n        //用于submit、reset事件\n        const isForm = this.getParent('fui-form');\n        if (isForm) {\n            const form = this.fuiForm;\n            form.$data['formChildren'].push(this);\n        }\n    },\n    methods: {\n        radioChange(e) {\n            this.$emit('change', e);\n            this.$emit(\"update:modelValue\", e);\n        },\n        changeValue(value, target) {\n            if (value == this.val)\n                return null;\n            this.val = value;\n            this.childrens.forEach((item) => {\n                if (item !== target) {\n                    item.$data['val'] = false;\n                }\n            });\n            this.radioChange(value);\n        },\n        modelChange(val) {\n            this.childrens.forEach((item) => {\n                if (item.$props['value'] == val) {\n                    item.$data['val'] = true;\n                }\n                else {\n                    item.$data['val'] = false;\n                }\n            });\n        },\n        getParent(name) {\n            if (this.$parent == null)\n                return false;\n            let parent = this.$parent;\n            let parentName = parent.$options['name'];\n            while (parentName != name) {\n                if (parent.$parent == null)\n                    return false;\n                parent = parent.$parent;\n                if (parent.$options['name'] == '')\n                    return false;\n                parentName = parent.$options['name'];\n            }\n            this.fuiForm = parent;\n            return true;\n        },\n        //暂时做重置处理（还原需记录初始值）\n        reset() {\n            this.val = '';\n            this.modelChange('');\n            this.radioChange('');\n        },\n        getSubmitValue() {\n            return this.val == '' ? this.modelValue : this.val;\n        }\n    }\n});\n//# sourceMappingURL=/Users/xyhc/Documents/veepai_uniappx/uni_modules/firstui-unix/components/fui-radio-group/fui-radio-group.uvue?vue&type=script&lang.uts.js.map","references":["/Applications/HBuilderX.app/Contents/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"fui-radio-group.uvue?vue&type=script&lang.uts.js\",\"sourceRoot\":\"\",\"sources\":[\"fui-radio-group.uvue?vue&type=script&lang.uts\"],\"names\":[],\"mappings\":\";AACC,OAAO,EAA2B,MAAM,KAAK,CAAA;AAC7C;;;;;;;;GAQG;AACH,+BAAe;IACd,IAAI,EAAE,iBAAiB;IACvB,KAAK,EAAE,CAAC,QAAQ,EAAE,mBAAmB,CAAC;IACtC,KAAK,EAAE;QACN,IAAI,EAAE;YACL,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,EAAE;SACX;QACD,UAAU,EAAE;YACX,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,EAAE;SACX;KACD;IACD,IAAI;QACH,OAAO;YACN,GAAG,EAAE,EAAE;YACP,SAAS,EAAE,EAA+B;YAC1C,OAAO,EAAE,IAAsC;SAC/C,CAAA;IACF,CAAC;IACD,KAAK,EAAE;QACN,UAAU,CAAC,GAAY;YACtB,IAAI,CAAC,WAAW,CAAC,GAAG,CAAC,CAAA;QACtB,CAAC;KACD;IACD,OAAO;QACN,kBAAkB;QAClB,MAAM,MAAM,GAAG,IAAI,CAAC,SAAS,CAAC,UAAU,CAAC,CAAC;QAC1C,IAAI,MAAM,EAAE;YACX,MAAM,IAAI,GAAG,IAAI,CAAC,OAAkC,CAAA;YACnD,I